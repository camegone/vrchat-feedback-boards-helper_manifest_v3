{
    "manifest_version": 3,
    "name": "VRChat Feedback Boards Helper",
    "description": "A helper tool for VRChat feedback boards on Canny.",
    "version": "1.0",
    "icons": {
        "128": "/images/3d_peng128.png"
    },
    "action": {
        "default_icon": {
            "128": "/images/3d_peng128.png"
        },
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": [
                "https://feedback.vrchat.com/*",
                "https://*.canny.io/*"
            ],
            "js": [
                "content_script_canny.js"
            ],
            "css": [
                "content_script_canny.css"
            ]
        }
    ]
}